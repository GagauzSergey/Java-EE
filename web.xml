<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>GetList</servlet-name>
		<servlet-class>ua.kiev.prog.GetListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetList</servlet-name>
		<url-pattern>/get</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Add</servlet-name>
		<servlet-class>ua.kiev.prog.AddServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Add</servlet-name>
		<url-pattern>/add</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>ua.kiev.prog.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Online</servlet-name>
		<servlet-class>ua.kiev.prog.OnlineUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Online</servlet-name>
		<url-pattern>/online</url-pattern>
</servlet-mapping>

	<servlet>
		<servlet-name>Registered</servlet-name>
		<servlet-class>ua.kiev.prog.RegisteredUsersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registered</servlet-name>
		<url-pattern>/registered</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>ChatRoomPost</servlet-name>
		<servlet-class>ua.kiev.prog.ChatRoomServletPost</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChatRoomPost</servlet-name>
		<url-pattern>/chat_room_post</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>ChatRoomGet</servlet-name>
		<servlet-class>ua.kiev.prog.ChatRoomServletGet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChatRoomGet</servlet-name>
		<url-pattern>/chat_room_get</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
